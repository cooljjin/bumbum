        },
        "emailInput": {
          "ref": "",
          "selector": "[data-testid=\"email\"]",
          "desc": "Email address input field"
        },
        "passwordInput": {
          "ref": "",
          "selector": "[data-testid=\"password\"]",
          "desc": "Password input field"
        }
      }
